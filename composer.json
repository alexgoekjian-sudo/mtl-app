{
  "name": "alexgoekjian-sudo/mtl-app",
  "description": "MTL_App minimal Lumen scaffold for Cloud86/Plesk deployment",
  "type": "project",
  "require": {
    "php": ">=8.0",
    "laravel/lumen-framework": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-root-package-install": ["@php -r \"file_exists('.env') || copy('.env.example', '.env');\""]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
