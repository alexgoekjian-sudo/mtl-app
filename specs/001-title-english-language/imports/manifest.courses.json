{
  "source": "Courses_from_Google_Sheets.csv",
  "mappings": {
    "COURSE_FULL_NAME": "course_full_name",
    "COURSE_SHORT_NAME": "course_key",
    "START_DATE": {"to": "start_date", "type": "date", "format": "M/D/YYYY"},
    "END_DATE": {"to": "end_date", "type": "date", "format": "M/D/YYYY"},
    "TIMES": "time_range",
    "DAYS": "days",
    "HRS": "hours_raw",
    "PRICE": {"to": "price", "type": "number"},
    "LOCATION": "location",
    "ONLINE_ONSITE": "delivery_mode",
    "REGISTRATION_LINK": "registration_link",
    "SCHEDULE_TYPE": "schedule_type",
    "SCHEDULE_NAME": "schedule_name"
  },
  "notes": "Parse START_DATE/END_DATE to ISO 8601. TIMES is a start-end string. DAYS is a comma-separated short-day list. COURSE_SHORT_NAME is used as canonical course_key when available."
}
